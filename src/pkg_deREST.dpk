package pkg_deREST;

{$R *.res}
{$IFDEF IMPLICITBUILDING This IFDEF should not be used by users}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION OFF}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES ON}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DEFINE DEBUG}
{$ENDIF IMPLICITBUILDING}
{$IMPLICITBUILD ON}

requires
  rtl,
  inet,
  dbrtl,
  FireDAC,
  FireDACCommonDriver,
  FireDACCommon;

contains
  deREST in 'units\deREST.pas',
  deREST.restapi in 'units\implementation\deREST.restapi.pas',
  deREST.datasets in 'units\implementation\deREST.datasets.pas',
  deREST.restobject in 'units\interface\deREST.restobject.pas',
  deREST.restcollection in 'units\interface\deREST.restcollection.pas',
  deREST.restresponse in 'units\interface\deREST.restresponse.pas',
  deREST.filterparser in 'units\implementation\deREST.filterparser.pas',
  deREST.restfilter in 'units\interface\deREST.restfilter.pas',
  deREST.restfilter.standard in 'units\implementation\deREST.restfilter.standard.pas',
  deREST.restresponse.standard in 'units\implementation\deREST.restresponse.standard.pas',
  deREST.restcollection.standard in 'units\implementation\deREST.restcollection.standard.pas',
  deREST.restobject.standard in 'units\implementation\deREST.restobject.standard.pas';

end.
